<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="400dp"
    android:background="@color/layer7"
    android:padding="2dp"
    android:orientation="vertical">
    <LinearLayout android:orientation="vertical"
        android:layout_height="match_parent"
        android:layout_weight="14"
        android:layout_width="match_parent">

        <LinearLayout android:orientation="horizontal"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_width="match_parent">
            <TextView
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:padding="3dp"
android:textSize="11sp"
                android:layout_height="match_parent"
                android:textColor="@color/colorblue_dark"
                android:text="@string/voucherNo"/>
            <EditText
                android:layout_width="match_parent"
                android:id="@+id/vochernum"
                android:layout_weight="1"
                android:textSize="11sp"
                android:enabled="false"
                android:layout_gravity="center"
                android:padding="3dp"
                android:textColor="@color/colorBlack"

                android:layout_height="match_parent"
                 />
            <TextView
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:textSize="11sp"
                android:padding="3dp"
                android:layout_height="match_parent"
                android:textColor="@color/colorblue_dark"
                android:text="@string/CUS_NAME"/>

            <EditText
                android:id="@+id/customername"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:enabled="false"
                android:padding="3dp"
                android:textColor="@color/colorBlack"
                android:textSize="11sp" />

        </LinearLayout>
        <LinearLayout android:orientation="horizontal"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_width="match_parent">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textSize="11sp"
                android:padding="3dp"
                android:textColor="@color/colorblue_dark"
                android:text="@string/voucher_date"/>
            <EditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textSize="11sp"
                android:padding="3dp"
                android:enabled="false"
                android:id="@+id/vocherdate"
                android:textColor="@color/colorBlack"

                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textSize="11sp"
                android:padding="3dp"
                android:textColor="@color/colorblue_dark"
                android:text="@string/pay_method"/>
            <EditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:enabled="false"
                android:textSize="11sp"
                android:padding="3dp"
                android:layout_gravity="center"
                android:id="@+id/paymethod"
                android:textColor="@color/colorBlack"

                />

        </LinearLayout>
    </LinearLayout>

    <HorizontalScrollView
        android:id="@+id/HorizontalScrollView01"
        android:layout_width="match_parent"
        android:layout_margin="5dp"
        android:layout_weight="7"
        android:fillViewport="true"
        android:layout_height="match_parent">
    <LinearLayout android:orientation="vertical"
        android:layout_height="match_parent"
        android:layout_width="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:layout_margin="5dp"
        android:orientation="horizontal"
        android:background="@color/colorblue_dark"
        android:paddingStart="5dp"
        android:paddingLeft="5dp">

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/item_number"
            android:textColor="@color/white"
            android:textStyle="bold" />

        <TextView
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/item_name"
            android:textColor="@color/white"
            android:textStyle="bold" />

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/unit_qty"
            android:textColor="@color/white"
            android:textStyle="bold" />

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/total_bonus_qty"
            android:textColor="@color/white"
            android:textStyle="bold" />
        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/SALEprice"
            android:textColor="@color/white"
            android:textStyle="bold" />
        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/originalprice"
            android:textColor="@color/white"
            android:textStyle="bold" />
        <TextView
            android:layout_width="90dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/total_sales_noTax"
            android:textColor="@color/white"
            android:textStyle="bold" />
    </LinearLayout>

        <LinearLayout android:orientation="vertical"
            android:layout_height="match_parent"
            android:layout_width="match_parent">



            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/layer7"

                android:layout_margin="5dp"
                >

                <TableLayout
                    android:id="@+id/TableItemsInfo1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/layer7">

                </TableLayout>
            </ScrollView>
        </LinearLayout>


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/layer7"

        android:layout_margin="5dp"
        >

        <TableLayout
            android:id="@+id/TableItemsInfo1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/layer7">

        </TableLayout>

    </ScrollView>

    </LinearLayout>

    </HorizontalScrollView>


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:visibility="gone"
        android:layout_gravity="center">

        <Button
            android:id="@+id/button11"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_margin="5dp"
            android:imeOptions="actionDone"
            android:background="@drawable/done_button"
            android:textColor="@color/layer5"
            android:text="@string/app_done"
            android:textStyle="bold"
            tools:ignore="TextViewEdits" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:background="@color/layer3"
        android:layout_margin="5dp"
        android:orientation="horizontal"

        android:paddingStart="5dp"
        android:paddingLeft="5dp">

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/SumSALEprice"
            android:textColor="@color/colorblue1"
            android:textStyle="bold" />
        <TextView
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:gravity="center"
        android:id="@+id/SALEprice"
        android:textColor="@color/colorblue1"
        android:textStyle="bold" />
        <TextView
            android:layout_width="30dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
        android:visibility="invisible"
            android:textColor="@color/colorblue1"
            android:textStyle="bold" />
        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/Sumoriginalprice"
            android:textColor="@color/colorblue1"
            android:textStyle="bold" />
        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:id="@+id/originalprice"
            android:textColor="@color/colorblue1"
            android:textStyle="bold" />
        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/deffirentPrice2"
            android:textColor="@color/colorblue1"
            android:textStyle="bold" />
        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:id="@+id/deff"

            android:textColor="@color/colorblue1"
            android:textStyle="bold" />
    </LinearLayout>
    <HorizontalScrollView

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"

        android:fillViewport="true">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/layer3"
        android:layout_margin="5dp"
        >




        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight=".9"
            android:textSize="12sp"
            android:text="@string/sub_total"
            android:paddingLeft="10dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/subTotalTextView"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:paddingLeft="5dp"
            android:textSize="12sp"
            android:text="@string/app_zero"
            android:textColor="@color/colorblue1"

            android:textStyle="bold" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@string/tax"
            android:textSize="12sp"
            android:paddingLeft="10dp"
            android:textStyle="bold" />


        <TextView
            android:id="@+id/taxTextView"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:paddingLeft="5dp"
            android:text="@string/app_zero"
            android:textColor="@color/colorblue1"
            android:textSize="12sp"
            android:textStyle="bold" />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight=".9"
            android:paddingLeft="10dp"
            android:text="@string/net_sales"
            android:textSize="12sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/netSalesTextView1"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:paddingLeft="5dp"
            android:text="@string/app_zero"
            android:textColor="@color/colorblue1"
            android:textSize="12sp"
            android:textStyle="bold" />

    </LinearLayout>
    </HorizontalScrollView>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
android:layout_weight=".5"
        android:layout_margin="5dp"
        >
        <Button
            android:id="@+id/Print"
            android:layout_width="80dp"
            android:text="@string/print"
            android:layout_height="match_parent"/>
    </LinearLayout>
    </LinearLayout>