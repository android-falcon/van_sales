        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/img"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                app:srcCompat="@drawable/ic_launcher_foreground"
                tools:visibility="visible" />

            <TextView
                android:id="@+id/compname"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:layout_margin="10dp"
                android:gravity="center"
                android:text="الصقور للبرمجيات "
                android:textColor="@color/add_button"
                android:textSize="18sp"
                android:textStyle="bold"
                tools:visibility="visible" />
            <TextView
                android:id="@+id/titla"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:layout_margin="10dp"
                android:gravity="center"
                android:text="@string/inventory_report"
                android:textColor="@color/add_button"
                android:textSize="18sp"
                android:textStyle="bold"
                tools:visibility="visible" />

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:padding="5dp">

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_marginBottom="3dp">

                    <TextView
                        android:id="@+id/textView1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="هاتف :"
                        android:textColor="@color/add_button"
                        android:textSize="18sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/tel"
                        android:layout_width="110dp"
                        android:layout_height="wrap_content"

                        android:textColor="@color/add_button"
                        android:textSize="18sp"
                        />

                    <TextView
                        android:id="@+id/textView15"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="الرقم الضريبي : "
                        android:textColor="@color/add_button"
                        android:textSize="18sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/taxNo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:textColor="@color/add_button"
                        android:textSize="18sp" />

                </TableRow>

                <TextView
                    android:id="@+id/textView644"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="________________________________________"
                    android:textColor="@color/add_button"


                    android:textSize="18sp"
                    android:visibility="gone" />


                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginBottom="3dp">



                    <TextView
                        android:id="@+id/date2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="التاريخ :"
                        android:textColor="@color/add_button"
                        android:textSize="18sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/date"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="@color/add_button"
                        android:textSize="16sp"
                        android:textStyle="bold" />

                </TableRow>


            </TableLayout>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_margin="3dp">

                <TextView
                    android:id="@+id/eda"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ملاحظة : "
                    android:textColor="@color/add_button"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/note"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/add_button"
                    android:textSize="18sp"
                    android:textStyle="bold" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="3dp">

                <TextView
                    android:id="@+id/name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="اسم المندوب  :  "
                    android:textColor="@color/add_button"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/salesman_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:textColor="@color/add_button"
                    android:textSize="18sp"
                    android:textStyle="bold" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="3dp">

                <TextView
                    android:id="@+id/qtyType"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="الكميات : "
                    android:textColor="@color/add_button"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/qtyTypeText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:textColor="@color/add_button"
                    android:textSize="18sp"
                    android:textStyle="bold" />

            </TableRow>
            <TextView
                android:id="@+id/textView26"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="       __________________________________________________________"
                android:textColor="@color/add_button"
                android:textSize="15sp"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_height="wrap_content"
                android:layoutDirection="rtl"
                android:layout_width="match_parent">
                <TextView
                    android:id="@+id/textView_itemName"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_margin="2dp"
                    android:layout_weight="1"
                    android:visibility="visible"
                    android:gravity="center"
                    android:textColor="@color/add_button"
                    android:textSize="18sp"
                    android:textStyle="bold"
                    android:text="@string/item_name"
                    />


                <TextView
                    android:id="@+id/textView_itemNumber"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_margin="2dp"
                    android:layout_weight="1"
                    android:textColor="@color/add_button"
                    android:textSize="18sp"
                    android:gravity="center"
                    android:text="@string/item_number"
                    android:textStyle="bold" />


                <TextView
                    android:id="@+id/textView_itemQuantity"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_margin="2dp"
                    android:layout_weight="1"
                    android:textColor="@color/add_button"
                    android:textSize="18sp"
                    android:gravity="center"
                    android:text="@string/unit_qty"
                    android:textStyle="bold" />
            </LinearLayout>
            <TextView
                android:id="@+id/textView264"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_gravity="center"
                android:text="       __________________________________________________________"
                android:textColor="@color/add_button"
                android:textSize="15sp"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/textView26431"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:visibility="invisible"
                android:layout_gravity="center"
                android:text="       __________________________________________________________"
                android:textColor="@color/add_button"
                android:textSize="15sp"
                android:textStyle="bold" />


        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/printFooter"
        android:layout_width="500dp"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:layoutDirection="rtl"
        android:visibility="visible"
        android:orientation="vertical"
        android:padding="5dp">
        <TextView
            android:id="@+id/textView264321"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:visibility="visible"
            android:layout_gravity="center"
            android:text="       __________________________________________________________"
            android:textColor="@color/add_button"
            android:textSize="15sp"
            android:textStyle="bold" />
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginBottom="3dp">

            <TextView
                android:id="@+id/total"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=" اجمالي الكمية  :   "
                android:textColor="@color/add_button"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/totalQty_Text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:textColor="@color/add_button"
                android:textSize="18sp"
                android:textStyle="bold" />

        </TableRow>
        <TextView
            android:id="@+id/textView2643"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:visibility="invisible"
            android:layout_gravity="center"
            android:text="       __________________________________________________________"
            android:textColor="@color/add_button"
            android:textSize="15sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView226413"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:visibility="invisible"
            android:layout_gravity="center"
            android:text="       __________________________________________________________"
            android:textColor="@color/add_button"
            android:textSize="15sp"
            android:textStyle="bold" />
        <TextView
            android:id="@+id/textView2264123"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:visibility="invisible"
            android:layout_gravity="center"
            android:text="       __________________________________________________________"
            android:textColor="@color/add_button"
            android:textSize="15sp"
            android:textStyle="bold" />

    </LinearLayout>
</LinearLayout>
