<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <HorizontalScrollView
        android:id="@+id/HorizontalScrollView01"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:layout_weight="1"

        android:fillViewport="true"


        android:layout_margin="2dp"

        >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:layout_margin="5dp"
        android:id="@+id/lin"
        android:orientation="horizontal">


            <TextView
                android:id="@+id/SE_voucherdate"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
              android:layout_marginRight="5dp"
                android:layout_marginLeft="5dp"
                android:gravity="center"
                android:text="voucher_date"
                android:textColor="@color/colorblue_dark"
                android:textSize="12sp"
                android:textStyle="bold" />


        <TextView
            android:id="@+id/SE_vocherkind3"
            android:layout_width="80dp"
            android:layout_height="match_parent"

            android:layout_weight="1"
            android:gravity="center"
            android:layout_marginRight="2dp"
            android:layout_marginLeft="2dp"
            android:visibility="gone"
            android:text="kind"
            android:textColor="@color/colorblue_dark"
            android:textStyle="bold" />

            <TextView
                android:id="@+id/SE_vocherkind"
                android:layout_width="120dp"
                android:layout_height="wrap_content"

                android:gravity="center"
                android:layout_marginRight="5dp"
                android:layout_marginLeft="5dp"
                android:text="فاتورة بيع"
                android:textColor="@color/colorblue_dark"
                android:textStyle="bold" />
        <TextView
            android:id="@+id/SE_vocherkind2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"

            android:layout_weight="1"
            android:gravity="center"
            android:layout_marginRight="2dp"
            android:layout_marginLeft="2dp"
            android:visibility="gone"
            android:text="kind"
            android:textColor="@color/colorblue_dark"
            android:textStyle="bold" />




            <TextView
                android:id="@+id/SE__serialcode"
                android:layout_width="170dp"
                android:layout_height="wrap_content"

                android:gravity="center"
                android:text="123456789011"
                android:layout_marginRight="5dp"
                android:layout_marginLeft="5dp"

                android:textColor="@color/colorblue_dark"
                android:textStyle="bold" />



            <TextView
                android:id="@+id/SE__itemnum"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:layout_marginRight="5dp"
                android:layout_marginLeft="5dp"
                android:text="item_number"
                android:textColor="@color/colorblue_dark"
                android:textStyle="bold" />



            <TextView
        android:id="@+id/SE_vochernum"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
        android:gravity="center"
                android:layout_marginRight="5dp"
                android:layout_marginLeft="5dp"
        android:text="voucherNo"
        android:textColor="@color/colorblue_dark"
        android:textStyle="bold" />

        <TextView
            android:id="@+id/SE_customerNo"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:layout_marginRight="5dp"
            android:layout_marginLeft="5dp"
            android:text="customerNo"
            android:textColor="@color/colorblue_dark"
            android:textStyle="bold" />
        <TextView
            android:id="@+id/SE_customerName"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_weight="1.5"
            android:layout_marginRight="5dp"
            android:layout_marginLeft="5dp"
            android:gravity="center"
            android:text="CustomerNmae241054"
            android:textColor="@color/colorblue_dark"
            android:textStyle="bold" />


<!--        <TextView-->
<!--            android:id="@+id/SE_exportexcel"-->
<!--            android:layout_width="match_parent"-->
<!--            android:layout_height="20dp"-->

<!--            android:layout_weight="1"-->
<!--            android:visibility="invisible"-->


<!--            android:background="@drawable/excel_e"-->
<!--            android:textSize="14sp" />-->




    </LinearLayout>
    </HorizontalScrollView>

</LinearLayout>